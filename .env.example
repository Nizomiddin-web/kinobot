# Bot Token from @BotFather
BOT_TOKEN=your_bot_token_here

# Database Type: postgresql or mysql
DATABASE_TYPE=postgresql

# Database URL
# PostgreSQL: postgresql+asyncpg://user:password@localhost:5432/kino_bot
# MySQL: mysql+aiomysql://user:password@localhost:3306/kino_bot
DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/kino_bot

# Movie Channel ID (where videos are stored)
# Get it by forwarding a message from the channel to @userinfobot
MOVIE_CHANNEL_ID=-1001234567890

# Super Admin IDs (comma separated)
SUPER_ADMIN_IDS=123456789,987654321
